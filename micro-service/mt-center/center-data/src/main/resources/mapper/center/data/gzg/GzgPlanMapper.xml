<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace = "com.ly.mt.center.data.gzg.mapper.GzgPlanMapper" >

    <select id="getGzgPlan" parameterType="com.ly.mt.center.data.gzg.entity.GzgPlan" resultType="com.ly.mt.center.data.gzg.entity.GzgPlan">
        select id, plan_name, one, two, three, four, five, six, seven
        from gzg_plan
        where id = #{id}
     </select>

</mapper >