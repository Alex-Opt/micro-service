<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace = "com.ly.mt.center.data.gzg.mapper.GzgSuitBarcodeInfoMapper" >

    <!-- 查询 -->
    <select id="queryBySuitSpuId" parameterType="java.lang.Long" resultType="java.lang.String">
        SELECT barcode  FROM gzg_suit_barcode_info
        WHERE suit_spu_id = #{spu_id}
    </select>
</mapper>